public class FindMinimum extends ConsoleProgram
{
    public void run()
    {
        int num1 = readInt("Number 1: ");
        int num2 = readInt("Number 2: ");
        int num3 = readInt("Number 3: ");
       
    if(num1 >= num2)
    {
    if(num2 >= num3)
    {
       System.out.println(num3);
    }
    }
   
    if(num2 >= num1)
    {
       if(num3 >= num1)
       {
           System.out.println(num1);
       }
    }
    else
    System.out.println(num2);
   
       
    }
}
